{
	"name": "知识库",
	"options": null,
	"author": "落雨不悔",
	"open_source": 1,
	"csv_url": "https://gitee.com/qiaoshengda/knowledge-base",
	"os_summary": "Apache-2.0",
	"site": "https://blog.esion.xyz",
	"package": "xyz.esion.knowledge-base",
	"preload": "",
	"debug_url": null,
	"url": "https://blog.esion.xyz/apps/knowledge-base",
	"logo": "./dist/logo.png",
	"description": "构建自己的知识库，高效便捷的知识管理工具",
	"type": "web",
	"auth": {
		"base": {
			"webSecure": true
		},
		"api": {
			"enable": false
		},
		"ability": {}
	},
	"window": {
		"defaultType": "frameWindow",
		"frameWindow": {
			"enable": true,
			"width": 1000,
			"height": 800,
			"controllers": {
				"goBack": true,
				"goForward": true,
				"refresh": true,
				"home": true
			},
			"canResize": true,
			"minWidth": "",
			"minHeight": ""
		},
		"window": {
			"enable": false,
			"width": 400,
			"minWidth": 300,
			"maxWidth": 600,
			"canResize": true,
			"height": 400,
			"minHeight": 300,
			"maxHeight": 600,
			"top": false,
			"blurAction": "none"
		},
		"attach": {
			"enable": false,
			"width": 320,
			"minWidth": 320,
			"maxWidth": 320,
			"canResize": true
		}
	},
	"theme_color": "#FFBA40",
	"file_assign": "",
	"modules": {
		"main": {
			"entry": "http://localhost:5173/index.html",
			"width": "1280px",
			"height": "800px"
		},
		"card": [{
			"name": "待办组件",
			"entry": "http://localhost:5173/todo.html",
			"size": "4x4",
			"preview": "./dist/logo.png"
		}, {
			"name": "笔记组件",
			"entry": "http://localhost:5173/note.html",
			"size": "4x4",
			"preview": "./dist/logo.png"
		}]
	}
}
